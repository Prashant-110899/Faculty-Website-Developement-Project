<!-- Header ejs -->
<%- include('./partials/header',{headerTitle:'AWARDS & ACADEMICS'})%>

<br><br><br><br><br>


<%- include("./partials/notes/noteDetails"); %>

<br>

<div class="container">
  <div class="all-posts-container">
    <% if(newItems) { %>

      <% newItems.forEach((item, i) => { %>
    
      <form class="item" action="/dashboard/awards-and-academics/delete" method="post">
    
        <%- include("./partials/posts/delete",{item:item}); %>
    
      </form>
    
      <% }); %>
      <% } else { %>
        <div class="container text-center">
          <h5>No notes to show. Enter subject and details to show up on the screen.</h5>
        </div>
      <% } %>
  </div>
    
      <!-- Modal -->
      <form class="item" action="/dashboard/awards-and-academics/edit" method="post">
    
        <%- include("./partials/posts/edit"); %>
    
      </form>
    
      <br>

      <form class="item" action="/dashboard/awards-and-academics" method="post">

        <%- include("./partials/posts/generic"); %>

      </form>
</div>

<hr>
